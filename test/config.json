{
  "rabbitmq": {
    "host": "localhost",
    "port": 5672
  },
  "redis": {
    "host": "localhost",
    "port": 6379
  },
  "syslog": {
    "host": "localhost",
    "port": 514
  },
  "api": {
    "host": "localhost",
    "port": 8080
  },
  "handlers": {
    "default": "ruby ./handler.rb"
  },
  "checks": {
    "foo": {
      "command": "ruby ./plugin.rb -c :::name:::",
      "subscribers": [
        "foo"
      ],
      "interval": 5,
      "alert": false
    },
    "bar": {
      "command": "ruby ./plugin.rb -c :::name:::",
      "subscribers": [
        "bar"
      ],
      "interval": 12,
      "alert": true
    }
  },
  "client": {
    "name": "i-424242",
    "address": "127.0.0.1",
    "subscriptions": [
      "foo",
      "bar"
    ]
  }
}
