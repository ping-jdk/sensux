{
  "rabbitmq": {
    "server": "localhost"
  },
  "redis": {
    "server": "localhost",
    "port": 6379
  },
  "api": {
    "server": "localhost",
    "port": 8080
  },
  "handlers": {
    "pagerduty": "ruby ./handlers/pagerduty"
  },
  "checks": {
    "fitter_happier": {
      "command": "cat 'fitter_happier'",
      "subscribers": [
        "webserver"
      ],
      "interval": 10,
      "alert": false
    },
    "cluster_health": {
      "command": "rm /tmp/thiswillfail",
      "subscribers": [
        "elasticsearch_ebs"
      ],
      "interval": 15,
      "alert": true
    }
  },
  "client": {
    "name": "i-424242",
    "address": "127.0.0.1",
    "subscriptions": [
      "webserver",
      "elasticsearch_ebs"
    ]
  }
}
