    "cluster_health": {
      "command": "echo 'cluster_health'",
      "subscribers": [
        "elasticsearch_ebs"
      ],
      "interval": 15,
      "alert": true
    }
  },
  "client": {
    "subscriptions": [
      "webserver",
      "elasticsearch_ebs"
    ],
    "details": {
      "name": "i-424242",
      "address": "127.0.0.1"
    }
  }
}
