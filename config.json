{
  "servers": [
    "localhost"
  ],
  "rabbitmq_server": "localhost",
  "subscriptions": [
    "webserver",
    "elasticsearch_ebs"
  ],
  "exchanges": [
    "webserver",
    "elasticsearch_ebs"
  ],
  "checks": {
    "fitter_happier": {
      "command": "echo 'fitter_happier' >> /tmp/sa-monitoring",
      "roles": [
        "webserver"
      ]
    },
    "cluster_health": {
      "command": "echo 'cluster_health' >> /tmp/sa-monitoring",
      "roles": [
        "elasticsearch_ebs"
      ]
    }
  }
}
