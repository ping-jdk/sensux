{
  "workers": [
    "localhost"
  ],
  "rabbitmq": {
    "server": "localhost"
  },
  "redis": {
    "server": "localhost"
  },
  "checks": {
    "fitter_happier": {
      "command": "echo 'fitter_happier'",
      "subscribers": [
        "webserver"
      ],
      "interval": 10,
      "alert": false
