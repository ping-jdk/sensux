{
  "rabbitmq": {
    "server": "localhost"
  },
  "redis": {
    "server": "localhost",
    "port": 6379
  },
  "api": {
    "server": "localhost",
    "port": 8080
  },
  "handlers": {
    "pagerduty": "ruby ./handlers/pagerduty"
  },
  "checks": {
    "foo": {
      "command": "ruby ./plugins/test",
      "subscribers": [
        "foo"
      ],
      "interval": 5,
      "alert": false
    },
    "bar": {
      "command": "ruby ./plugins/test",
      "subscribers": [
        "bar"
      ],
      "interval": 12,
      "alert": true
    }
  },
  "client": {
    "name": "i-424242",
    "address": "127.0.0.1",
    "subscriptions": [
      "foo",
      "bar"
    ]
  }
}
