{
  "rabbitmq": {
    "server": "localhost",
    "vhost": "/sa-monitoring",
    "username": "user",
    "password": "pass"
  },
  "exchanges": [
    "webserver",
    "elasticsearch_ebs"
  ],
  "checks": {
    "fitter_happier": {
      "command": "echo 'fitter_happier'",
      "roles": [
        "webserver"
      ]
    },
    "cluster_health": {
      "command": "echo 'cluster_health'",
      "roles": [
        "elasticsearch_ebs"
      ]
    }
  }
}
